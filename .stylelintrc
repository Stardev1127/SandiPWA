{
  "plugins": [
    "stylelint-scss",
    "stylelint-order"
  ],
  "fix": true,
  "extends": [
      "stylelint-config-standard"
  ],
  "rules": {
    "indentation": 4,
    "number-leading-zero": "never",
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "at-rule-empty-line-before": null,
    "order/order": [
      "custom-properties",
      "declarations",
      "rules"
    ]
  }
}
